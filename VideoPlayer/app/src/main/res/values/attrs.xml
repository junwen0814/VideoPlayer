<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- FunGameHeader -->
    <declare-styleable name="FunGameHeader">
        <attr name="game_type" format="enum">
            <enum name="hit_block" value="0" />
            <enum name="battle_city" value="1" />
        </attr>
    </declare-styleable>

    <!-- FunGameView -->
    <declare-styleable name="FunGame">
        <attr name="left_model_color" format="color" />
        <attr name="middle_model_color" format="color" />
        <attr name="right_model_color" format="color" />
    </declare-styleable>

    <!-- HitBlockView -->
    <declare-styleable name="HitBlock">
        <attr name="block_horizontal_num" format="integer" />
        <attr name="ball_speed" format="integer">
            <enum name="low" value="3" />
            <enum name="medium" value="6" />
            <enum name="fast" value="9" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CustomProgressBar">
        <attr name="progressColor" format="color"/>
        <attr name="progressDescColor" format="color"/>
        <attr name="progressRadius" format="dimension"/>
        <attr name="max" format="integer"/>
        <attr name="progress" format="integer"/>
        <attr name="borderWidths" format="dimension"/>
        <attr name="progressDesc" format="string"/>
        <attr name="isShowDesc" format="boolean"/>
    </declare-styleable>
</resources>